{
  "name": "js-starter",
  "version": "0.0.1",
  "description": "js starter",
  "repository": {
    "type": "git",
    "url": "git://github.com/giovanettid/js-starter.git"
  },
  "scripts": {
    "start": "webpack-dev-server --hot",
    "server:build": "npm run build && browser-sync start -s $npm_package_config_buildDir",
    "precommit": "run-p lint coverage",
    "lint": "run-p lint:*",
    "lint:js": "eslint . src test --ext .js",
    "lint:sass": "sass-lint -v -q",
    "test": "jest --config jest.config.json",
    "coverage": "jest --config jest.config.json --coverage",
    "e2e": "wdio test/e2e/wdio.conf.js",
    "quality": "plato -t 'project-title' -d report -r src/",
    "prebuild": "rm -rf $npm_package_config_buildDir && mkdir -p $npm_package_config_buildDir",
    "build": "NODE_ENV=production webpack --config webpack.config.js -p"
  },
  "author": "Didier Giovanetti",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "cucumber": "^3.0.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "plato": "^1.7.0",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^2.0.8",
    "raf": "^3.4.0",
    "sass-lint": "^1.11.1",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.1",
    "style-loader": "^0.19.0",
    "wdio-cucumber-framework": "^1.0.2",
    "wdio-selenium-standalone-service": "0.0.9",
    "wdio-spec-reporter": "^0.1.2",
    "webdriverio": "^4.8.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.3"
  },
  "config": {
    "buildDir": "build"
  }
}
