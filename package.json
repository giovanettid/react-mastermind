{
  "name": "js-starter",
  "version": "1.7.0",
  "description": "js starter",
  "repository": {
    "type": "git",
    "url": "git://github.com/giovanettid/js-starter.git"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "server:build": "npm run build && browser-sync start -s $npm_package_config_buildDir",
    "lint": "run-p lint:*",
    "lint:js": "eslint . src test --ext .js,.jsx",
    "lint:sass": "stylelint 'src/**/*.scss'",
    "test": "jest --config jest.config.json",
    "coverage": "jest --config jest.config.json --coverage",
    "e2e": "wdio run test/e2e/wdio.conf.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "prepare": "husky install"
  },
  "author": "Didier Giovanetti",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.6.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@cucumber/cucumber": "^9.0.0",
    "@wdio/cli": "^7.30.2",
    "@wdio/cucumber-framework": "^7.30.2",
    "@wdio/local-runner": "^7.30.2",
    "@wdio/selenium-standalone-service": "^7.30.2",
    "@wdio/spec-reporter": "^7.30.2",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "browser-sync": "^2.28.3",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^2.7.3",
    "mkdirp": "^1.0.3",
    "multiple-cucumber-html-reporter": "^3.2.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.0.2",
    "raf": "^3.4.1",
    "sass-loader": "^13.2.0",
    "sinon": "^8.1.1",
    "stylelint": "^15.2.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "uuid": "^3.4.0",
    "wdio-cucumberjs-json-reporter": "^4.4.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "config": {
    "buildDir": "build"
  }
}
