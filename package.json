{
  "name": "mastermind",
  "version": "0.0.1",
  "description": "mastermind",
  "repository": {
    "type": "git",
    "url": "git://github.com/giovanettid/mastermind.git"
  },
  "scripts": {
    "start": "webpack-dev-server --hot",
    "server:build": "npm run build && browser-sync start -s $npm_package_config_buildDir",
    "lint": "run-p lint:*",
    "lint:js": "eslint . src test --ext .js,.jsx",
    "lint:sass": "stylelint 'src/**/*.scss' --syntax scss",
    "test": "jest --config jest.config.json",
    "coverage": "jest --config jest.config.json --coverage",
    "e2e": "wdio run test/e2e/wdio.conf.js",
    "quality": "es6-plato -t 'project-title' -d report -r src/",
    "build": "NODE_ENV=production webpack --config webpack.config.js -p --display-modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p lint coverage"
    }
  },
  "author": "Didier Giovanetti",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.6.4",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@wdio/cli": "^5.18.5",
    "@wdio/cucumber-framework": "^5.16.15",
    "@wdio/local-runner": "^5.18.5",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.18.5",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.18.13",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "cucumber": "^6.0.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "es6-plato": "^1.2.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "raf": "^3.4.0",
    "sass-loader": "^8.0.2",
    "sinon": "^8.1.1",
    "stylelint": "^13.0.0",
    "stylelint-config-sass-guidelines": "^6.2.0",
    "stylelint-scss": "^3.14.0",
    "uuid": "^3.4.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "config": {
    "buildDir": "build"
  }
}
