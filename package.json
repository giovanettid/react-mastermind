{
  "name": "js-starter",
  "version": "1.6.0",
  "description": "js starter",
  "repository": {
    "type": "git",
    "url": "git://github.com/giovanettid/js-starter.git"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "server:build": "npm run build && browser-sync start -s $npm_package_config_buildDir",
    "lint": "run-p lint:*",
    "lint:js": "eslint . src test --ext .js,.jsx",
    "lint:sass": "stylelint 'src/**/*.scss' --syntax scss",
    "test": "jest --config jest.config.json",
    "coverage": "jest --config jest.config.json --coverage",
    "e2e": "wdio run test/e2e/wdio.conf.js",
    "quality": "es6-plato -t 'project-title' -d reports/quality -r src/",
    "build": "NODE_ENV=production webpack --config webpack.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p lint coverage"
    }
  },
  "author": "Didier Giovanetti",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.6.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@wdio/cli": "^5.18.5",
    "@wdio/cucumber-framework": "^5.16.15",
    "@wdio/local-runner": "^5.18.5",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.18.5",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^9.1.2",
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "cucumber": "^6.0.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "es6-plato": "^1.2.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^2.7.3",
    "mkdirp": "^1.0.3",
    "multiple-cucumber-html-reporter": "^1.14.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.0.2",
    "raf": "^3.4.1",
    "sass-loader": "^13.2.0",
    "sinon": "^8.1.1",
    "stylelint": "^12.0.1",
    "stylelint-config-sass-guidelines": "^6.2.0",
    "stylelint-scss": "^3.14.2",
    "uuid": "^3.4.0",
    "wdio-cucumberjs-json-reporter": "^1.1.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "config": {
    "buildDir": "build"
  }
}
